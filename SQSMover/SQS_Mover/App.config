<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
 <appSettings>
      <!-- Source Amazon Credentials and Queue -->
      <add key="SourceAWSAccessKey" value="YOUR ACCESS KEY HERE" />
      <add key="SourceAWSSecretKey" value="YOUR SECRET KEY HERE" />
      <add key="SourceQueue" value="NAME_OF_THE_SOURCE_QUEUE"/>
   
      <!-- Target Amazon Credentials and Queue -->
      <add key="TargetAWSAccessKey" value="YOUR ACCESS KEY HERE" />
      <add key="TargetAWSSecretKey" value="YOUR SECRET KEY HERE" />   
      <add key="TargetQueue" value="NAME_OF_THE_TARGET_QUEUE" />
   
      <!-- Amazon queue details -->
      <add key="AWSProfileName" value="" />

      <add key="LogThreshold"        value="10000" />
      <add key="MessagesPerDequeue"  value="100" />
      
    </appSettings>
</configuration>